package week02

object exercise3 {
  def product(f: Int => Int)(a: Int, b: Int): Int = {
  	def loop(a: Int, acc: Int): Int = {
  		if(a > b) acc
  		else loop(a + 1, acc * f(a))
  	}
  	loop(a, 1)
  }                                               //> product: (f: Int => Int)(a: Int, b: Int)Int
  product(x => x)(2, 5)                           //> res0: Int = 120
}